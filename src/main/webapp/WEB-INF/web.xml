<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.sistemaparqueadero.Servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>vehiculoServlet</servlet-name>
        <servlet-class>com.sistemaparqueadero.Servlet.vehiculoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MotoServlet</servlet-name>
        <servlet-class>com.sistemaparqueadero.Servlet.MotoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BicicletaServlet</servlet-name>
        <servlet-class>com.sistemaparqueadero.Servlet.BicicletaServlet</servlet-class>
    </servlet>
    
    
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>vehiculoServlet</servlet-name>
        <url-pattern>/vehiculoServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
     
        
</web-app>
